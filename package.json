{
  "name": "angular2-webpack",
  "version": "0.0.9",
  "description": "Angular2, Webpack2, Material Design, Typescript2, Docker, Sass, Codelyzer",
  "license": "MIT",
  "main": "",
  "engines": {
    "node": ">= 4.4.7",
    "npm": ">= 3"
  },
  "scripts": {
    "initialize": "docker-compose build",
    "clean": "rimraf dist/frontend",
    "tslint": "tslint ./src/frontend/app/**/*.ts",
    "serve": "webpack-dev-server --config webpack/webpack.dev.js --inline --progress --profile --watch --content-base src/frontend --host=0.0.0.0",
    "build": "npm run clean && npm run aot &&  webpack --config webpack/webpack.prod.js  --progress --profile",
    "serve:dist": "http-server dist/frontend  -p 8080 ",
    "docker:install": "docker-compose run frontend npm install",
    "docker:serve:dist": "docker-compose run --service-ports frontend npm run serve-dist",
    "docker:build": "docker-compose run frontend npm run build",
    "docker:serve": "docker-compose run --service-ports frontend npm run serve",
    "docker:tslint": "docker-compose run --service-ports frontend npm run tslint",
    "docker:bash": "docker-compose run --service-ports frontend bash",
    "aot": "./node_modules/.bin/ngc -p tsconfig.aot.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/compiler-cli": "0.6.3",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/platform-server": "2.0.1",
    "@angular/router": "3.0.0",
    "@angular/material": "2.0.0-alpha.9-3",
    "@types/core-js": "0.9.28",
    "@types/hammerjs": "2.0.28",
    "@types/node": "4.0.29",
    "@types/source-map": "0.1.26",
    "@types/uglify-js": "2.0.27",
    "@types/webpack": "^1.12.34",
    "angular2-template": "0.0.1",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "2.2.4",
    "codelyzer": "0.0.28",
    "compression-webpack-plugin": "0.3.1",
    "copy-webpack-plugin": "2.1.6",
    "core-js": "2.4.1",
    "css-loader": "0.25.0",
    "es6-promise": "3.3.1",
    "es6-promise-loader": "1.0.2",
    "es7-reflect-metadata": "1.6.0",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "file-loader": "0.9.0",
    "hammerjs": "2.0.8",
    "html-webpack-plugin": "2.22.0",
    "http-server": "0.9.0",
    "ie-shim": "0.1.0",
    "ignore-loader": "0.1.1",
    "image-webpack-loader": "2.0.0",
    "imports-loader": " 0.6.5",
    "json-loader": "0.5.4",
    "less": "2.5.3",
    "less-loader": "2.2.2",
    "node-sass": "3.10.0",
    "normalize.css": " 4.1.1",
